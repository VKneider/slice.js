{
    "version":2,
    "builds":[
        {
            "src":"/Slice/server.js",
            "use":"@vercel/node"
        },
        {
          "src": "/Slice/**/*",
          "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/Slice/server.js"
        }
    ]
}