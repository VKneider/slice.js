{
  "debugger": {
    "enabled": true,
    "click": "right"
  },
  "stylesManager": {
    "defaultTheme": "Dark"
  },
  "logger": {
    "enabled": true,
    "showLogs": {
      "console": {
        "error": true,
        "warning": true,
        "info": true
      },
      "toast": {
        "error": true,
        "warning": true,
        "info": true
      }
    }
  },
  "translator": {
    "enabled": true
  },
  "paths": {
    "components": "./Components",
    "themes": "./Themes",
    "styles": "./Styles"
  }
}
