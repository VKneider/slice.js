{
    "debugger": {
        "enabled": false,
        "click": "right"
    },
    "themeManager": {
        "defaultTheme": "Slice",
        "saveThemeLocally": true,
        "useBrowserTheme": false
    },
    "logger": {
        "enabled": false,
        "showLogs": {
            "console": {
                "error": true,
                "warning": true,
                "info": false
            }
        }
    },
    "translator": {
        "enabled": false
    },
    "paths": {
        "components": "/Slice/Components",
        "themes": "/Slice/Themes",
        "styles": "/Slice/Styles",
        "userComponents": "/Components"
    },
    "router":{
        "defaultRoute": "/",
        "enabled": true
        
    },
    "loading":{
        "enabled": true
    }
}


